{
  "name": "slack-clone-server",
  "version": "1.0.0",
  "description": "slack clone with node.js",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/server.js",
    "seed:dev": "sequelize db:seed:undo:all --seeders-path src/db/seeders/development && sequelize db:seed:all --seeders-path src/db/seeders/development",
    "test": "export NODE_ENV=test; npm run db:drop && npm run db:setup && jest --detectOpenHandles --forceExit",
    "db:setup": "sequelize db:create && sequelize db:migrate",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)"
    ]
  },
  "author": "yshir",
  "license": "MIT",
  "dependencies": {
    "ajv": "^6.12.3",
    "bcrypt": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "morgan": "^1.10.0",
    "pg": "^8.3.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.3",
    "sequelize-cli": "^6.2.0",
    "socket.io": "^2.5.0",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "supertest": "^4.0.2"
  }
}
